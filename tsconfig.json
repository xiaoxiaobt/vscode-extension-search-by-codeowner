{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "outDir": "out",
    "lib": [
      "esnext"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    ".vscode-test",
    "media"
  ]
}
